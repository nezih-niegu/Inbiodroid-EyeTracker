<launch> 
    <!-- By default USB0 is the board port, if is not give it as argument
    example : roslaunch ojos eyecontrol.launch tcp_ip:=1                    -->
    
    <arg name="tcp_ip" default="0"/>

    <node pkg="rosserial_python" type="serial_node.py" name="serial_eye_control">
        <param name="port" value="/dev/ttyUSB$(arg tcp_ip)" />
        <param name="baud" value="256000" />
    </node>
</launch>
