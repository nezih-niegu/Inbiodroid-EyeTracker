<launch> 
    <!-- launchfile to run two topics, each one in a different transmission -->
    <node pkg="ojos" type="cameraShowImageStream.py" name="left_camera_image" output="screen" args="left"/>
    <node pkg="ojos" type="cameraShowImageStream2.py" name="right_camera_image" output="screen" args="right"/>

    <node pkg="web_video_server" type="web_video_server" name="webrtc" output="screen" args="_ros_threads:=3 _port:=25564"/>
    
</launch>
